{
  "name": "quiz-night",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "tailwind:base": "tailwind build src/styles/tailwind.css -o src/styles/base.css -c tailwind.config.js",
    "tailwind:build": "npm run clean && npm run tailwind:base",
    "dev-notw": "next dev",
    "dev": "yarn run tailwind:build && next dev",
    "build": "yarn run tailwind:build && next build",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint --ext .js,.jsx --max-warnings=0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/alert-dialog": "^0.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "7.0.0",
    "firebase": "^7.15.3",
    "framer-motion": "^1.11.1",
    "i18next": "^19.5.1",
    "js-cookie": "^2.2.1",
    "nanoid": "^3.1.10",
    "next": "9.4.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-feather": "^2.0.8",
    "react-firebase-hooks": "^2.2.0",
    "react-hook-form": "^6.0.0",
    "react-i18next": "^11.7.0",
    "slugify": "^1.4.0",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-transform-react-jsx": "^7.10.3",
    "@svgr/webpack": "^5.4.0",
    "css-loader": "^3.6.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "twin.macro": "^1.4.1"
  }
}
